package Vtalent.com.Feb11;
import java.util.Scanner;
public class factorial {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        long factorial1 = 1;
        for(int i=1;i<=num;i++){
            factorial1 *= i;
        }
        System.out.println("factorial of " + num + " is: " + factorial1);
        sc.close();
    }
}
